#include <stdio.h>
#include <stdlib.h>
#include <string.h>

// Функция, преобразующая двоичную строку в десятичное число.
// Принимает на вход указатель на строку, представляющую двоичное число.
int binaryToDecimal(char *binaryString) {
    int length = strlen(binaryString); // Получаем длину строки
    int decimal = 0; // Инициализируем десятичное число нулем
    // Перебираем каждый символ в строке.
    for (int i = 0; i < length; i++) {
        if (binaryString[i] == '1') { // Если символ '1', добавляем 1 к десятичному числу
            decimal = decimal * 2 + 1;
        } else if (binaryString[i] == '0') { // Если символ '0', не меняем десятичное число
            decimal = decimal * 2;
        } else { // Если символ не '0' и не '1', выводим сообщение об ошибке и завершаем программу
            printf("Error: invalid binary string.\n");
            exit(1);
        }
    }
    return decimal; // Возвращаем десятичное число
}

// Основная функция программы.
int main() {
    char binaryString[9] = "11001100"; // Создаем массив символов и заполняем его двоичным числом.
    int decimal = binaryToDecimal(binaryString); // Вызываем функцию binaryToDecimal для преобразования двоичной строки в десятичное число.
    printf("%d", decimal); // Выводим десятичное число в консоль.
    return 0;
}
